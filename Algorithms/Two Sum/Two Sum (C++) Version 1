class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> temp;
        
		temp.push_back(nums[0]);
		
        for(int i = 1; i < nums.size(); i++)
        {
			for(int j = 0; j < i; j++)
			{
				if((temp[j]+nums[i]) == target)
					return {i,j};
			}
			
			temp.push_back(nums[i]);
        }
        
        return {0,0};
    }
};
